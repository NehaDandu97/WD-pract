/* h3{
    color: brown;
}
p{
    color: plum;
    background-color: black;
}
button{
    color: blueviolet;
    background-color: burlywood;
}
body{
    background-color:#ff00ff;
} */
/* rgb(255,0, 0) , #ff0000-red
rgb(0, 255, 0) , #00ff00-green
rgb(0, 0, 255) , #0000ff-blue
rgb(255, 255, 255) , #ffffff-white
rgb(0, 0, 0) , #000000-black 
rgb(255, 255, 0) , #ffff00-yellow
rgb(0, 255, 255) , #00ffff-light blue
rgb(255, 0, 255) , #ff00ff-pink */

/* universal selector */
/* *{
    color: #ff00ff;
} */
/* element selector */
/* h3{
    color: brown;
} */
/* ID selector */
/* #id1{
    color: brown;
} */
/* Class selector */
/* .c1{
    color: blueviolet;
} */


/* Text properties */
/* h1{
    text-align: left;
}
h2{
    text-align: right;
}
h3{
    text-align: center;
}
#id1{
    text-align: start;
}
#id2{
    text-align: end;
} */


/* Text Decoration */
/* h1{
    text-decoration: underline;
}
h2{
    text-decoration: overline;
}
h3{
    text-decoration: line-through;
}
h4{
    text-decoration: red wavy underline;
}
p{
    text-decoration: green dotted underline;
}
a{
    text-decoration: none;
} */


/* Font Weight-100 to 900 (normal,lighter,bold,bolder) */
/* #id1{
    font-weight: normal;
}
#id2{
    font-weight: lighter;
}
#id3{
    font-weight: bold;
}
#id4{
    font-weight: bolder;
} */

/* Font Family - can give one style or multiple styles(if one style fails other works)*/
/* #id201{
    font-family: arial;

}
#id202{
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
} */

/*Font Size-units */
/* #id301{
    font-size: 25px;
}
#id302{
    font-size: small;
}
#id303{
    font-size:large;
} */

/*Line Height */
/* #id401{
    line-height: 60px;
} */

/* Text Transform */
/* #id501{
    text-transform: capitalize;
} */


/* Height and Width*/
/* div{
    width: 400px;
    height: 100px;
    background-color: blue;
}
h1{
    width: 200px;
    height: 50px;
    background-color: chartreuse;
} */

/* Border */
/* h1 {
    width: 200px;
    height: 50px;
    background-color: chartreuse;
    border: 7px solid black;
    text-align: center;
}

div {
    width: 400px;
    height: 100px;
    background-color: gray;
    border-width: 6px;
    border-style: dashed;
    border-color: yellow;
} */

/* Border-radius - both height and width are equal and radius set 50% - circle */

/* div {
    width: 400px;
    height: 100px;
    background-color: gray;
    border: 5px solid purple;
    border-radius: 10px;
} */

/* div {
    width: 100px;
    height: 100px;
    background-color: gray;
    border: 5px solid purple;
    border-radius: 50%;
} */

/* Padding */
/* div {
    width: 100px;
    height: 100px;
    background-color: gray;
    border: 5px solid purple;
    padding-left: 25px;
    padding-right: 25px;
    padding-bottom: 25px;
    padding-top: 25px;

} */

/* div {
    width: 100px;
    height: 100px;
    background-color: gray;
    border: 5px solid purple;
    padding: 50px;

} */

/* div {
    width: 100px;
    height: 100px;
    background-color: gray;
    border: 5px solid purple;
    padding: 1px 2px 3px 4px;

} */

/* Margin */
/* div{
    width: 100px;
    height: 100px;
    background-color: gray;
    border: 5px solid purple;
    padding: 50px;
    margin: 25px;
} */

/* Display property */
/* INLINE : takes only space requied (no margin/padding)
BLOCK : takes full space avaiable in width
INLINE - BLOCK : similar to inline but we can set margin and padding
NONE : to remove element from document flow(no space reserved) */


/* convert block element to inline -occupy requied area*/
/* div{
    background-color: pink;
    display: inline;
} */

/* convert inline element to block -occupy full line space*/
/* button{
    display: block;
} */

/* div{
    width: 100px;
    height: 100px;
    margin: 25px;
    padding: 25px;
    display: inline-block;
} */

/* #box2{
    display: none;
} */

/* Visibility - remove the element but reserves the space*/
/* #box2{
    visibility: hidden;
} */

/* Alpha channel - Opacity(0-1) */

/* div{
    width: 100px;
    height: 100px;
    margin: 25px;
    padding: 25px;
    display: inline-block;
}
#box1{
    background-color: rgba(0, 0, 255, 0.5);
}
#box2{
    background-color: rgba(0, 0, 255, 0.75);
}
#box3{
    background-color: rgba(0, 0, 255, 1);
} */

/* Units - Relative(changes) */
/* % */
/* #box1{
    height: 100px;
    width: 100px;
    background-color: cadetblue;
}
#box2{
    height: 50px;
    width: 30%;
    background-color: blueviolet;
    margin-left: 10%;
} */

/* em - 2em-two times of parent's font size , 0.5 half of parent's font size */
